# mujin_coreConfig.cmake â€” generated by configure_package_config_file
# Provides the mujin::mujin_core imported target for use by the ros2/ package.
@PACKAGE_INIT@

# PACKAGE_PREFIX_DIR is set by @PACKAGE_INIT@ to the root of the install tree.
find_library(MUJIN_CORE_LIBRARY
    NAMES mujin_core
    PATHS "${PACKAGE_PREFIX_DIR}/lib"
    NO_DEFAULT_PATH
    REQUIRED
)
find_library(LAPKT_CORE_LIBRARY
    NAMES lapkt_core
    PATHS "${PACKAGE_PREFIX_DIR}/lib"
    NO_DEFAULT_PATH
    REQUIRED
)
find_library(BTCPP_LIBRARY
    NAMES behaviortree_cpp
    PATHS "${PACKAGE_PREFIX_DIR}/lib"
    NO_DEFAULT_PATH
    REQUIRED
)

if(NOT TARGET mujin::mujin_core)
    add_library(mujin::lapkt_core STATIC IMPORTED GLOBAL)
    set_target_properties(mujin::lapkt_core PROPERTIES
        IMPORTED_LOCATION "${LAPKT_CORE_LIBRARY}"
    )

    add_library(mujin::behaviortree_cpp STATIC IMPORTED GLOBAL)
    set_target_properties(mujin::behaviortree_cpp PROPERTIES
        IMPORTED_LOCATION "${BTCPP_LIBRARY}"
        INTERFACE_INCLUDE_DIRECTORIES "${PACKAGE_PREFIX_DIR}/include"
    )

    add_library(mujin::mujin_core STATIC IMPORTED GLOBAL)
    set_target_properties(mujin::mujin_core PROPERTIES
        IMPORTED_LOCATION "${MUJIN_CORE_LIBRARY}"
        INTERFACE_INCLUDE_DIRECTORIES "${PACKAGE_PREFIX_DIR}/include"
        INTERFACE_LINK_LIBRARIES "mujin::lapkt_core;mujin::behaviortree_cpp"
    )
endif()
